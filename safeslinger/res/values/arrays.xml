<resources>

    <!-- 'Me, Myself, ...': this is a list of single words that are not real names for people, and should be filtered out to prevent people from exchanging names referring to themselves in the third person. There is no size restriction to this list. -->
    <string-array name="invalid_contact_names" translatable="false">
        <item>Me</item>
        <item>Myself</item>
        <item>Name</item>
        <item>My</item>
        <item>Contact</item>
        <item>ICE</item>
        <item>I.C.E.</item>
        <item>Test</item>
        <item>Sample</item>
        <item>Person</item>
    </string-array>

    <!-- NOTE: if you change anything about this array, you must make the corresponding change to pass_phrase_cache_ttl_values below. -->
